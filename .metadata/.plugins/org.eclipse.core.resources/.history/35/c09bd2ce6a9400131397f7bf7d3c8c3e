public class Solution {
    public void merge(int A[], int m, int B[], int n) {
        int all = m+n-1;
        int a=m-1;
        int b=n-1;
        while(a>=0 && b>=0){
            int tempA = A[a];
            int tempB = B[b];
            if(tempA > tempB){
                A[all] = A[a];
                a--;
                all--;
            }
            else{
                A[all] = B[b];
                b--;
                all--;
            }
        }
        if(b>=0){
            for(int i=b; i>=0; i--){
                A[i] = B[i];
            }
        }
    }
}